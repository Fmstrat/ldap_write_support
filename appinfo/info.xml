<?xml version="1.0"?>
<info xmlns:xsi= "http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="https://apps.nextcloud.com/schema/apps/info.xsd">
    <id>user_ldap_extended</id>
    <name>Ldap User Extended</name>
    <summary>Creates users on LDAP database instead of NextCloud</summary>
    <description><![CDATA[This app implements the following actions to an LDAP backend: create/delete user, modify display name, avatar and e-mail.]]></description>
    <licence>agpl</licence>
    <author mail="alan@eita.org.br" homepage="http://eita.org.br">Alan Freihof Tygel</author>
    <version>0.0.6</version>
    <namespace>Ldapusermanagement</namespace>
    <category>auth</category>
    <bugs>https://gitlab.com/eita/ldapusermanagement/issues</bugs>
    <dependencies>
        <user_ldap />
        <owncloud min-version="9.1" max-version="9.1"/>
        <nextcloud min-version="10" max-version="13"/>
    </dependencies>
    <install>
        <step>OCA\Ldapusermanagement\Validate</step>
    </install>
</info>
